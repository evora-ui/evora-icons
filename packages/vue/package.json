{
  "name": "@evora-ui/icons-vue",
  "version": "1.0.0",
  "description": "1500+ free SVG icon components for Vue 3 — line & filled variants. Tree‑shakable, typed, CSP/SSR‑friendly.",
  "type": "module",
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "author": "Evora UI Authors",
  "typesVersions": {
    "*": {
      "icons": [
        "src/icons/index.d.ts"
      ],
      "names": [
        "src/names.ts",
        "dist/names.d.ts"
      ],
      "icons/names": [
        "src/names.ts",
        "dist/names.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./icons": {
      "types": "./src/icons/index.d.ts",
      "default": "./dist/icons/index.js"
    },
    "./icons/names": {
      "types": "./dist/icons/names.d.ts",
      "default": "./dist/icons/names.js"
    },
    "./names": {
      "types": "./dist/icons/names.d.ts",
      "default": "./dist/icons/names.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src/icons/index.d.ts",
    "src/names.ts",
    "README.md",
    "LICENSE",
    "NOTICE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "yarn clean && yarn generate",
    "build": "rollup -c",
    "pretypes": "yarn generate",
    "types": "tsc -p tsconfig.json --emitDeclarationOnly --declaration && cp -f src/global-components.d.ts dist/global-components.d.ts",
    "generate": "node ../../tools/generate-icons.mjs --pkg packages/vue",
    "copy:legal": "cp -f ../../LICENSE ./LICENSE && cp -f ../../NOTICE ./NOTICE",
    "prepack": "yarn copy:legal",
    "prepublishOnly": "yarn clean && yarn generate && yarn build && yarn types && yarn lint:fix && yarn copy:legal",
    "lint": "eslint 'src/**/*.{ts,js,vue}'",
    "lint:fix": "eslint 'src/**/*.{ts,js,vue}' --fix"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "esbuild": "^0.23.0",
    "rollup": "^4.22.4",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "~5.9.2",
    "vue": "^3.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue",
    "icons",
    "svg",
    "components",
    "line icons",
    "filled icons",
    "evora",
    "evora-ui",
    "vue3",
    "vue-icons",
    "icon set",
    "free icons"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evora-ui/evora-icons.git",
    "directory": "packages/vue"
  },
  "bugs": {
    "url": "https://github.com/evora-ui/evora-icons/issues"
  },
  "homepage": "https://github.com/evora-ui/evora-icons/tree/main/packages/vue#readme",
  "engines": {
    "node": ">=18"
  }
}
